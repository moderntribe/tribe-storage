{
  "name": "moderntribe/tribe-storage",
  "description": "A WordPress plugin to upload files to many different storage types like s3, Azure, DO Spaces",
  "type": "wordpress-plugin",
  "authors": [
    {
      "name": "Modern Tribe",
      "email": "admin@tri.be"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.3",
    "php-di/php-di": "^6.0",
    "league/flysystem-azure-blob-storage": "^1.0",
    "league/flysystem-cached-adapter": "^1.1",
    "intervention/image": "^2.5",
    "symfony/lock": "^5.2",
    "jhofm/flysystem-iterator": "^2.2"
  },
  "require-dev": {
    "brain/monkey": "2.*",
    "phpunit/phpunit": "^9",
    "php-mock/php-mock-mockery": "^1.3",
    "moderntribe/coding-standards": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Tribe\\Storage\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tribe\\Storage\\Tests\\": "tests/"
    },
    "files": [
      "tests/Bootstrap.php"
    ]
  },
  "config": {
    "platform": {
      "php": "7.3.0"
    }
  }
}
